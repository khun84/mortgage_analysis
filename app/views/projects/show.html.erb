<%= render partial: 'project', locals: {project: @project} %>

<%= form_for :project, url: edit_project_path(@project.id), method: :get do |f| %>
    <%= f.submit 'Edit project' %>
<% end %>

<%= form_for :project, url: project_path(@project.id), method: :delete do |f| %>
    <%= f.submit 'Delete project', data: { confirm: 'Are you sure?'} %>
<% end %>

<%= link_to 'New scenario', new_project_scenario_path(@project.id) %>