
<table id="forex-table" class="table">
    <thead>
        <tr>
            <th>Currency</th>
            <th>Rate</th>
        </tr>
    </thead>

    <tbody>
        <% rates.each do |cur, rate| %>
        <tr>
            <td><%= cur %></td>
            <td><%= rate %></td>
        </tr>
        <% end %>
    </tbody>

</table>
